<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="onufreiv.cassiopeia">

	<uses-permission android:name="android.permission.BLUETOOTH" />
	<uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".activity.PocMainActivity" />
		<activity
			android:name=".activity.MainActivity"
			android:theme="@style/AppTheme.NoActionBar">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.VuMeterActivity" />
		<activity android:name=".activity.RainbowActivity" />
		<activity android:name=".activity.StripsActivity"></activity>
		<activity android:name=".activity.RunningFrequenciesActivity"></activity>
		<activity android:name=".activity.SpectrumAnalyzerActivity"></activity>
	</application>

</manifest>